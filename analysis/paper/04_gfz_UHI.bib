@article{locke2016,
  title = {What’s Scale Got to Do with It? {{Models}} for Urban Tree Canopy},
  shorttitle = {What’s Scale Got to Do with It?},
  author = {Locke, Dexter H. and Landry, Shawn M. and Grove, J. Morgan and Roy Chowdhury, Rinku},
  date = {2016},
  journaltitle = {Journal of Urban Ecology},
  shortjournal = {J Urban Ecol},
  volume = {2},
