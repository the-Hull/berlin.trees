@article{begum2013,
  title = {Regulation of Cambial Activity in Relation to Environmental Conditions: Understanding the Role of Temperature in Wood Formation of Trees},
  shorttitle = {Regulation of Cambial Activity in Relation to Environmental Conditions},
  author = {Begum, Shahanara and Nakaba, Satoshi and Yamagishi, Yusuke and Oribe, Yuichiro and Funada, Ryo},
  date = {2013},
  journaltitle = {Physiologia Plantarum},
  volume = {147},
  number = {1},
  pages = {46--54},
  issn = {1399-3054},
  doi = {10.1111/j.1399-3054.2012.01663.x},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1399-3054.2012.01663.x},
  urldate = {2021-10-15},
  abstract = {The timing of cambial reactivation plays an important role in determination of the amount and quality of wood and the environmental adaptivity of trees. Environmental factors, such as temperature, influence the growth and development of trees. Temperatures from late winter to early spring affect the physiological processes that are involved in the initiation of cambial cell division and xylem differentiation in trees. Cumulative elevated temperatures from late winter to early spring result in earlier initiation of cambial reactivation and xylem differentiation in tree stems and an extended growth period. However, earlier cambial reactivation increases the risk for frost damage because the cold tolerance of cambium decreases after cambial reactivation. The present review focuses on temperature regulation on the timing of cambial reactivation and xylem differentiation in trees, and also highlights recent advances in our understanding of seasonal changes in the cold stability of microtubules in trees. The review also summarizes the present understanding of the relationships between the timing of cambial reactivation, the start of xylem differentiation and changes in levels of storage materials in trees, as well as an attempt to identify the source of energy for cell division and differentiation. A better understanding of the mechanisms that regulate wood formation in trees and the influence of environmental conditions on such mechanisms should help in efforts to improve and enhance the exploitation of wood for commercial applications and to prepare for climatic change.},
  langid = {english},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1399-3054.2012.01663.x},
  file = {C\:\\Users\\ahurl\\transfer\\OneDrive\\_literature\\begum_et_al_2013_regulation_of_cambial_activity_in_relation_to_environmental_conditions.pdf;C\:\\Users\\ahurl\\Zotero\\storage\\KP7R7LDN\\j.1399-3054.2012.01663.html}
}

@article{bialecki2018,
