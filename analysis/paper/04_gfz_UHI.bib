@incollection{nowak2020,
  title = {Valuing {{Urban Tree Impacts}} on {{Precipitation Partitioning}}},
  booktitle = {Precipitation {{Partitioning}} by {{Vegetation}}: {{A Global Synthesis}}},
  author = {Nowak, David J. and Coville, Robert and Endreny, Theodore and Abdi, Reza and Van Stan II, John T.},
