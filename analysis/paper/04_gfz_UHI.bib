@article{tsonkova2012,
  title = {Ecological Benefits Provided by Alley Cropping Systems for Production of Woody Biomass in the Temperate Region: A Review},
  shorttitle = {Ecological Benefits Provided by Alley Cropping Systems for Production of Woody Biomass in the Temperate Region},
