  keywords = {Statistics - Computation,stats},
  file = {C\:\\Users\\ahurl\\Zotero\\storage\\C9BGPY9D\\Faulkenberry_2019_A tutorial on generalizing the default Bayesian t-test via posterior sampling.pdf;C\:\\Users\\ahurl\\Zotero\\storage\\QWEXVPJ3\\1812.html}
}

@article{fenner2014,
  title = {Spatial and Temporal Air Temperature Variability in {{Berlin}}, {{Germany}}, during the Years 2001–2010},
