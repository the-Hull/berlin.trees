  langid = {english},
  keywords = {boreal forest,carbon dioxide,productivity,stable isotopes,water use efficiency},
  file = {C\:\\Users\\ahurl\\Zotero\\storage\\QSNIIUFI\\Giguère-Croteau et al. - 2019 - North America’s oldest boreal trees are more effic.pdf;C\:\\Users\\ahurl\\Zotero\\storage\\WC7I4VPX\\2749.html}
