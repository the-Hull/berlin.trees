@article{briber2015,
  title = {Tree {{Productivity Enhanced}} with {{Conversion}} from {{Forest}} to {{Urban Land Covers}}},
  author = {Briber, Brittain M. and Hutyra, Lucy R. and Reinmann, Andrew B. and Raciti, Steve M. and Dearborn, Victoria K. and Holden, Christopher E. and Dunn, Allison L.},
  date = {2015-08-24},
  journaltitle = {PLOS ONE},
