  keywords = {Absorption of solar radiation,Analysis,Environmental conditions,Forestry,LANDSAT,Normalized difference vegetation index,Ratio vegetation indices,Remote sensing,Satellite imagery,Urban vegetation,Vegetation,Vegetation index}
