  author = {Huang, Q. and Xu, H. and Yang, X. and Shi, P.},
  date = {2009},
  series = {American {{Society}} for {{Photogrammetry}} and {{Remote Sensing Annual Conference}} 2009, {{ASPRS}} 2009},
  volume = {1},
  pages = {300--305},
  url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84868583799&partnerID=40&md5=bac2d0f14d71b99a8e8fc0efbfd832f4},
  abstract = {Phenology is an indicator of the impact of climate on vegetation. As the representation of human activities, urbanization affects vegetation phenology by changing the local climate such as temperature, humidity, wind speed, etc. Recent studies starts to focus on the impacts of urbanization intensity on phenology. In order to understand the relationship between urbanization intensity and phenology, we identified the phenological transitional dates based on NDVI from SPOT/VGT and used a eveloped urban index ranges from 0-1 to represent intensity of urbanization using data from Defense Satellite Meteorological Program - Operational Linescan System (DMSP-OLS). The relationship between urban index and phenological transition dates from 1999 to 2002 were analyzed. It is found that there's a not-so significant linear relationship between urban index and phenological transitional dates anomaly. However, when the excluded the biophysical factors, the local climatic variation is the key factor determining the phenology anomaly, while urbanization level also has influence on it.},
  affiliation = {Department of Geography and Environmental Management, University of Waterloo, Waterloo, ON N2L 3G1, Canada; State Key Laboratory of Earth Surface Processes and Resource Ecology, Beijing Normal University, Beijing, 100875, China; Academy of Disaster Reduction and Emergency Management, Ministry of Civil Affairs, Ministry of Education of China, Beijing, 100875, China},
  document_type = {Conference Paper},
