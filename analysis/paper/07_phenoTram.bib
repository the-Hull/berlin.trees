  author = {Weinstein, Ben G. and Marconi, Sergio and Aubry‐Kientz, Mélaine and Vincent, Gregoire and Senyondo, Henry and White, Ethan P.},
  date = {2020},
  journaltitle = {Methods in Ecology and Evolution},
  volume = {11},
  number = {12},
  pages = {1743--1751},
