  document_type = {Article},
  langid = {chinese},
  pubmed_id = {18975761},
  source = {Scopus},
  keywords = {article,Asia,buffer zone,China,city planning,City Planning,classification,development and aging,ecosystem,Ecosystem,environmental monitoring,Environmental Monitoring,Eurasia,Far East,growth,NDVI,phenology,Poaceae,river,Rivers,tree,Trees,urban area,urbanization,Urbanization,vegetation,Yangtze Delta}
}

