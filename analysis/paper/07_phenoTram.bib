  title = {Determining the Effect of Green Spaces on {{Urban}} Heat Distribution Using Satellite Imagery},
  author = {Choi, H.-A. and Lee, W.-K. and Byun, W.-H.},
  date = {2012},
  journaltitle = {Asian Journal of Atmospheric Environment},
  volume = {6},
